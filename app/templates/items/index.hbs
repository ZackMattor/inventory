<div class="row">
  <div class="col-md-12">
    <table class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Name</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>
        {{#each item in model}}
          <tr>
            <td>{{item.name}}</td>
            <td>{{item.location}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
